{
  "name": "tohuwabohu",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "prebuild": "payload migrate && payload generate:types",
    "build": "next build",
    "postbuild": "node scripts/conditional-seed.js",
    "start": "next start",
    "lint": "payload generate:types && eslint .",
    "lint:fix": "payload generate:types && eslint --fix .",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "typecheck": "payload generate:types && tsc --noEmit",
    "update": "npx npm-check-updates -i",
    "update:semver": "npx npm-check-updates -i -t semver",
    "generate:types": "payload generate:types",
    "migrate": "payload migrate",
    "migrate:create": "payload migrate:create",
    "migrate:fresh": "payload migrate:fresh",
    "seed": "payload run ./src/seed/index.ts"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "^3.73.0",
    "@payloadcms/next": "^3.73.0",
    "@payloadcms/richtext-lexical": "^3.73.0",
    "@payloadcms/storage-vercel-blob": "^3.73.0",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "graphql": "^16.12.0",
    "lucide-react": "^0.562.0",
    "next": "^16.1.4",
    "payload": "^3.73.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "schema-dts": "^1.1.5",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^24.10.9",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.4",
    "eslint-config-prettier": "^10.1.8",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "typescript": "^5.9.3"
  },
  "browserslist": [
    "defaults and fully supports es6-module"
  ]
}
